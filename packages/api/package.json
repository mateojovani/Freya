{
  "name": "freya-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mateo Jovani",
  "license": "MIT",
  "scripts": {
    "start": "npm run build;npm run serve",
    "build": "tsc",
    "serve": "node -r dotenv/config ./dist/index.js",
    "start:dev": "concurrently -r \"npm:type-check:watch\" \"npm:watch\"",
    "watch": "nodemon --exec ts-node -r dotenv/config src/index.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "apollo-server-express": "^2.16.1",
    "express": "^4.17.1",
    "graphql": "^15.3.0"
  }
}
