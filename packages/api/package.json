{
  "name": "freya-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mateo Jovani",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:dev": "npm run docker; concurrently -r \"npm:build\" \"npm:type-check:watch\" \"npm:watch:debug\"",
    "build": "tsc --build",
    "serve": "node -r dotenv/config ./dist/index.js",
    "watch": "nodemon --exec ts-node -r dotenv/config src/index.ts",
    "watch:debug": "nodemon --exec node --inspect -r ts-node/register -r dotenv/config src/index.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "docker": "docker-compose up -d"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "@types/mongoose": "^5.7.36",
    "@types/puppeteer": "^3.0.1",
    "apollo-server-express": "^2.16.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.11.0",
    "mongoose": "^5.10.0",
    "pdf2pic": "^2.1.0",
    "puppeteer": "^5.2.1",
    "uuidv4": "^6.2.3"
  }
}
