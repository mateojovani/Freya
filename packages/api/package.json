{
  "name": "freya-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mateo Jovani",
  "license": "MIT",
  "scripts": {
    "start": "npm run build;npm run serve",
    "build": "tsc",
    "serve": "node ./dist/index.js",
    "start:dev": "concurrently -r \"npm:type-check:watch\" \"npm:watch\"",
    "watch": "nodemon --exec ts-node src/index.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0"
  }
}